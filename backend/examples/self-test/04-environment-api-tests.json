{
  "name": "Environment API Complete Test Suite (Multi-Tenant)",
  "version": "2.0.0",
  "description": "Complete test coverage for Environment API endpoints with multi-tenant support",
  "variables": {
    "baseUrl": "http://localhost:8090/api",
    "dbPath": "./data/test_management.db",
    "tenantId": "",
    "projectId": "",
    "envId": "",
    "testEnvId": "",
    "timestamp": ""
  },
  "steps": {
    "generateTimestamp": {
      "id": "generateTimestamp",
      "name": "Generate Unique IDs",
      "type": "script",
      "config": {
        "language": "python",
        "script": "import json\nimport datetime\nimport random\n\nts = datetime.datetime.now().strftime('%Y%m%d%H%M%S')\nrand_suffix = random.randint(1000, 9999)\n\nresult = {\n    'timestamp': f'{ts}{rand_suffix}',\n    'tenant_id': f'tenant-env-{ts}',\n    'project_id': f'project-env-{ts}',\n    'dev_env_id': f'dev-env-{ts}',\n    'test_env_id': f'test-env-{ts}'\n}\n\nprint(json.dumps(result))",
        "timeout": 5
      },
      "output": {
        "timestamp": "output.timestamp",
        "tenantId": "output.tenant_id",
        "projectId": "output.project_id",
        "devEnvId": "output.dev_env_id",
        "testEnvId": "output.test_env_id"
      }
    },

    "cleanupOldData": {
      "id": "cleanupOldData",
      "name": "Cleanup Old Test Data",
      "type": "database",
      "dependsOn": ["generateTimestamp"],
      "config": {
        "driver": "sqlite3",
        "dsn": "{{dbPath}}",
        "queryType": "exec",
        "query": "DELETE FROM environments WHERE env_id LIKE 'dev-env-%' OR env_id LIKE 'test-env-%'; DELETE FROM projects WHERE project_id LIKE 'project-env-%'; DELETE FROM tenants WHERE tenant_id LIKE 'tenant-env-%'"
      },
      "onError": "continue"
    },

    "createTenant": {
      "id": "createTenant",
      "name": "[DB] Create Test Tenant",
      "type": "database",
      "dependsOn": ["cleanupOldData"],
      "config": {
        "driver": "sqlite3",
        "dsn": "{{dbPath}}",
        "queryType": "exec",
        "query": "INSERT INTO tenants (tenant_id, name, display_name, description, status, max_projects, max_users, max_test_cases) VALUES (?, 'Environment Test Tenant', 'Test Tenant for Environment API', 'Automated test tenant', 'active', 10, 50, 1000)",
        "args": ["{{generateTimestamp.output.tenant_id}}"]
      }
    },

    "createProject": {
      "id": "createProject",
      "name": "[DB] Create Test Project",
      "type": "database",
      "dependsOn": ["createTenant"],
      "config": {
        "driver": "sqlite3",
        "dsn": "{{dbPath}}",
        "queryType": "exec",
        "query": "INSERT INTO projects (project_id, tenant_id, name, display_name, description, status) VALUES (?, ?, 'Environment Test Project', 'Test Project for Environment API', 'Automated test project', 'active')",
        "args": ["{{generateTimestamp.output.project_id}}", "{{generateTimestamp.output.tenant_id}}"]
      }
    },

    "createDevEnvironment": {
      "id": "createDevEnvironment",
      "name": "[API] Create Dev Environment",
      "type": "http",
      "dependsOn": ["createProject"],
      "config": {
        "method": "POST",
        "url": "{{baseUrl}}/environments",
        "headers": {
          "Content-Type": "application/json",
          "X-Tenant-ID": "{{generateTimestamp.output.tenant_id}}",
          "X-Project-ID": "{{generateTimestamp.output.project_id}}"
        },
        "body": {
          "env_id": "{{generateTimestamp.output.dev_env_id}}",
          "name": "Development Environment",
          "description": "Development environment for testing",
          "is_active": true,
          "config": {
            "baseUrl": "http://dev.example.com",
            "timeout": 5000
          },
          "variables": {
            "DB_HOST": "localhost",
            "DB_PORT": "3306",
            "API_KEY": "dev-api-key-123"
          }
        },
        "timeout": 5000
      },
      "output": {
        "devEnvId": "response.env_id"
      }
    },

    "assertDevEnvCreated": {
      "id": "assertDevEnvCreated",
      "name": "[Assert] Verify Dev Environment Created",
      "type": "assert",
      "dependsOn": ["createDevEnvironment"],
      "config": {
        "assertions": [
          {
            "type": "equals",
            "actual": "{{createDevEnvironment.status}}",
            "expected": 201,
            "message": "Create environment should return 201"
          },
          {
            "type": "exists",
            "actual": "{{createDevEnvironment.response.env_id}}",
            "message": "Response should contain env_id"
          },
          {
            "type": "equals",
            "actual": "{{createDevEnvironment.response.is_active}}",
            "expected": true,
            "message": "Environment should be active"
          },
          {
            "type": "equals",
            "actual": "{{createDevEnvironment.response.tenant_id}}",
            "expected": "{{generateTimestamp.output.tenant_id}}",
            "message": "Tenant ID should match"
          }
        ]
      }
    },

    "verifyDevEnvInDB": {
      "id": "verifyDevEnvInDB",
      "name": "[DB] Verify Dev Environment in Database",
      "type": "database",
      "dependsOn": ["assertDevEnvCreated"],
      "config": {
        "driver": "sqlite3",
        "dsn": "{{dbPath}}",
        "queryType": "select",
        "query": "SELECT env_id, name, is_active, tenant_id FROM environments WHERE env_id = ? AND tenant_id = ? AND deleted_at IS NULL",
        "args": ["{{generateTimestamp.output.dev_env_id}}", "{{generateTimestamp.output.tenant_id}}"]
      }
    },

    "assertDevEnvInDB": {
      "id": "assertDevEnvInDB",
      "name": "[Assert] Verify Dev Environment in DB",
      "type": "assert",
      "dependsOn": ["verifyDevEnvInDB"],
      "config": {
        "assertions": [
          {
            "type": "equals",
            "actual": "{{verifyDevEnvInDB.rowCount}}",
            "expected": 1,
            "message": "Should find environment in database"
          },
          {
            "type": "equals",
            "actual": "{{verifyDevEnvInDB.rows[0].is_active}}",
            "expected": 1,
            "message": "Environment should be active in DB"
          },
          {
            "type": "equals",
            "actual": "{{verifyDevEnvInDB.rows[0].tenant_id}}",
            "expected": "{{generateTimestamp.output.tenant_id}}",
            "message": "Tenant ID should match in DB"
          }
        ]
      }
    },

    "getDevEnvironment": {
      "id": "getDevEnvironment",
      "name": "[API] Get Dev Environment",
      "type": "http",
      "dependsOn": ["assertDevEnvInDB"],
      "config": {
        "method": "GET",
        "url": "{{baseUrl}}/environments/{{generateTimestamp.output.dev_env_id}}",
        "headers": {
          "Content-Type": "application/json",
          "X-Tenant-ID": "{{generateTimestamp.output.tenant_id}}",
          "X-Project-ID": "{{generateTimestamp.output.project_id}}"
        },
        "timeout": 5000
      }
    },

    "assertGetDevEnv": {
      "id": "assertGetDevEnv",
      "name": "[Assert] Verify Get Dev Environment",
      "type": "assert",
      "dependsOn": ["getDevEnvironment"],
      "config": {
        "assertions": [
          {
            "type": "equals",
            "actual": "{{getDevEnvironment.status}}",
            "expected": 200,
            "message": "Get environment should return 200"
          },
          {
            "type": "equals",
            "actual": "{{getDevEnvironment.response.env_id}}",
            "expected": "{{generateTimestamp.output.dev_env_id}}",
            "message": "Environment ID should match"
          },
          {
            "type": "exists",
            "actual": "{{getDevEnvironment.response.variables}}",
            "message": "Variables should exist"
          }
        ]
      }
    },

    "listEnvironments": {
      "id": "listEnvironments",
      "name": "[API] List Environments",
      "type": "http",
      "dependsOn": ["assertGetDevEnv"],
      "config": {
        "method": "GET",
        "url": "{{baseUrl}}/environments",
        "headers": {
          "Content-Type": "application/json",
          "X-Tenant-ID": "{{generateTimestamp.output.tenant_id}}",
          "X-Project-ID": "{{generateTimestamp.output.project_id}}"
        },
        "timeout": 5000
      }
    },

    "validateListEnvs": {
      "id": "validateListEnvs",
      "name": "[Script] Validate Environment List",
      "type": "script",
      "dependsOn": ["listEnvironments"],
      "config": {
        "language": "python",
        "script": "import json\n\nenvs_response = context.get('listEnvironments', {}).get('response', [])\ndev_env_id = context.get('generateTimestamp', {}).get('output', {}).get('dev_env_id')\n\nenv_found = any(env.get('env_id') == dev_env_id for env in envs_response)\n\nresult = {\n    'total_envs': len(envs_response),\n    'dev_env_found': env_found,\n    'has_environments': len(envs_response) > 0\n}\n\nprint(json.dumps(result))",
        "timeout": 5
      }
    },

    "assertListEnvs": {
      "id": "assertListEnvs",
      "name": "[Assert] Verify List Environments",
      "type": "assert",
      "dependsOn": ["validateListEnvs"],
      "config": {
        "assertions": [
          {
            "type": "equals",
            "actual": "{{listEnvironments.status}}",
            "expected": 200,
            "message": "List environments should return 200"
          },
          {
            "type": "equals",
            "actual": "{{validateListEnvs.output.dev_env_found}}",
            "expected": true,
            "message": "Dev environment should be found in list"
          }
        ]
      }
    },

    "getActiveEnvironment": {
      "id": "getActiveEnvironment",
      "name": "[API] Get Active Environment",
      "type": "http",
      "dependsOn": ["assertListEnvs"],
      "config": {
        "method": "GET",
        "url": "{{baseUrl}}/environments/active",
        "headers": {
          "Content-Type": "application/json",
          "X-Tenant-ID": "{{generateTimestamp.output.tenant_id}}",
          "X-Project-ID": "{{generateTimestamp.output.project_id}}"
        },
        "timeout": 5000
      }
    },

    "assertActiveEnv": {
      "id": "assertActiveEnv",
      "name": "[Assert] Verify Active Environment",
      "type": "assert",
      "dependsOn": ["getActiveEnvironment"],
      "config": {
        "assertions": [
          {
            "type": "equals",
            "actual": "{{getActiveEnvironment.status}}",
            "expected": 200,
            "message": "Get active environment should return 200"
          },
          {
            "type": "equals",
            "actual": "{{getActiveEnvironment.response.env_id}}",
            "expected": "{{generateTimestamp.output.dev_env_id}}",
            "message": "Active environment should be dev environment"
          }
        ]
      }
    },

    "getVariables": {
      "id": "getVariables",
      "name": "[API] Get Environment Variables",
      "type": "http",
      "dependsOn": ["assertActiveEnv"],
      "config": {
        "method": "GET",
        "url": "{{baseUrl}}/environments/{{generateTimestamp.output.dev_env_id}}/variables",
        "headers": {
          "Content-Type": "application/json",
          "X-Tenant-ID": "{{generateTimestamp.output.tenant_id}}",
          "X-Project-ID": "{{generateTimestamp.output.project_id}}"
        },
        "timeout": 5000
      }
    },

    "assertGetVariables": {
      "id": "assertGetVariables",
      "name": "[Assert] Verify Get Variables",
      "type": "assert",
      "dependsOn": ["getVariables"],
      "config": {
        "assertions": [
          {
            "type": "equals",
            "actual": "{{getVariables.status}}",
            "expected": 200,
            "message": "Get variables should return 200"
          },
          {
            "type": "exists",
            "actual": "{{getVariables.response.DB_HOST}}",
            "message": "DB_HOST variable should exist"
          },
          {
            "type": "equals",
            "actual": "{{getVariables.response.DB_HOST}}",
            "expected": "localhost",
            "message": "DB_HOST should match"
          }
        ]
      }
    },

    "getSpecificVariable": {
      "id": "getSpecificVariable",
      "name": "[API] Get Specific Variable",
      "type": "http",
      "dependsOn": ["assertGetVariables"],
      "config": {
        "method": "GET",
        "url": "{{baseUrl}}/environments/{{generateTimestamp.output.dev_env_id}}/variables/API_KEY",
        "headers": {
          "Content-Type": "application/json",
          "X-Tenant-ID": "{{generateTimestamp.output.tenant_id}}",
          "X-Project-ID": "{{generateTimestamp.output.project_id}}"
        },
        "timeout": 5000
      }
    },

    "assertGetSpecificVar": {
      "id": "assertGetSpecificVar",
      "name": "[Assert] Verify Get Specific Variable",
      "type": "assert",
      "dependsOn": ["getSpecificVariable"],
      "config": {
        "assertions": [
          {
            "type": "equals",
            "actual": "{{getSpecificVariable.status}}",
            "expected": 200,
            "message": "Get specific variable should return 200"
          },
          {
            "type": "equals",
            "actual": "{{getSpecificVariable.response.value}}",
            "expected": "dev-api-key-123",
            "message": "API_KEY value should match"
          }
        ]
      }
    },

    "setVariable": {
      "id": "setVariable",
      "name": "[API] Set Environment Variable",
      "type": "http",
      "dependsOn": ["assertGetSpecificVar"],
      "config": {
        "method": "PUT",
        "url": "{{baseUrl}}/environments/{{generateTimestamp.output.dev_env_id}}/variables/NEW_VAR",
        "headers": {
          "Content-Type": "application/json",
          "X-Tenant-ID": "{{generateTimestamp.output.tenant_id}}",
          "X-Project-ID": "{{generateTimestamp.output.project_id}}"
        },
        "body": {
          "value": "new-variable-value"
        },
        "timeout": 5000
      }
    },

    "assertSetVariable": {
      "id": "assertSetVariable",
      "name": "[Assert] Verify Set Variable",
      "type": "assert",
      "dependsOn": ["setVariable"],
      "config": {
        "assertions": [
          {
            "type": "equals",
            "actual": "{{setVariable.status}}",
            "expected": 200,
            "message": "Set variable should return 200"
          }
        ]
      }
    },

    "verifyNewVariable": {
      "id": "verifyNewVariable",
      "name": "[API] Verify New Variable Set",
      "type": "http",
      "dependsOn": ["assertSetVariable"],
      "config": {
        "method": "GET",
        "url": "{{baseUrl}}/environments/{{generateTimestamp.output.dev_env_id}}/variables/NEW_VAR",
        "headers": {
          "Content-Type": "application/json",
          "X-Tenant-ID": "{{generateTimestamp.output.tenant_id}}",
          "X-Project-ID": "{{generateTimestamp.output.project_id}}"
        },
        "timeout": 5000
      }
    },

    "assertNewVariable": {
      "id": "assertNewVariable",
      "name": "[Assert] Verify New Variable Value",
      "type": "assert",
      "dependsOn": ["verifyNewVariable"],
      "config": {
        "assertions": [
          {
            "type": "equals",
            "actual": "{{verifyNewVariable.status}}",
            "expected": 200,
            "message": "Get new variable should return 200"
          },
          {
            "type": "equals",
            "actual": "{{verifyNewVariable.response.value}}",
            "expected": "new-variable-value",
            "message": "New variable value should match"
          }
        ]
      }
    },

    "updateEnvironment": {
      "id": "updateEnvironment",
      "name": "[API] Update Environment",
      "type": "http",
      "dependsOn": ["assertNewVariable"],
      "config": {
        "method": "PUT",
        "url": "{{baseUrl}}/environments/{{generateTimestamp.output.dev_env_id}}",
        "headers": {
          "Content-Type": "application/json",
          "X-Tenant-ID": "{{generateTimestamp.output.tenant_id}}",
          "X-Project-ID": "{{generateTimestamp.output.project_id}}"
        },
        "body": {
          "name": "Updated Development Environment",
          "description": "Updated description for testing"
        },
        "timeout": 5000
      }
    },

    "assertUpdateEnv": {
      "id": "assertUpdateEnv",
      "name": "[Assert] Verify Update Environment",
      "type": "assert",
      "dependsOn": ["updateEnvironment"],
      "config": {
        "assertions": [
          {
            "type": "equals",
            "actual": "{{updateEnvironment.status}}",
            "expected": 200,
            "message": "Update environment should return 200"
          },
          {
            "type": "equals",
            "actual": "{{updateEnvironment.response.name}}",
            "expected": "Updated Development Environment",
            "message": "Updated name should match"
          }
        ]
      }
    },

    "verifyUpdateInDB": {
      "id": "verifyUpdateInDB",
      "name": "[DB] Verify Update in Database",
      "type": "database",
      "dependsOn": ["assertUpdateEnv"],
      "config": {
        "driver": "sqlite3",
        "dsn": "{{dbPath}}",
        "queryType": "select",
        "query": "SELECT name, description FROM environments WHERE env_id = ? AND tenant_id = ? AND deleted_at IS NULL",
        "args": ["{{generateTimestamp.output.dev_env_id}}", "{{generateTimestamp.output.tenant_id}}"]
      }
    },

    "assertUpdateInDB": {
      "id": "assertUpdateInDB",
      "name": "[Assert] Verify Database Update",
      "type": "assert",
      "dependsOn": ["verifyUpdateInDB"],
      "config": {
        "assertions": [
          {
            "type": "equals",
            "actual": "{{verifyUpdateInDB.rows[0].name}}",
            "expected": "Updated Development Environment",
            "message": "Database name should be updated"
          }
        ]
      }
    },

    "createTestEnvironment": {
      "id": "createTestEnvironment",
      "name": "[API] Create Test Environment",
      "type": "http",
      "dependsOn": ["assertUpdateInDB"],
      "config": {
        "method": "POST",
        "url": "{{baseUrl}}/environments",
        "headers": {
          "Content-Type": "application/json",
          "X-Tenant-ID": "{{generateTimestamp.output.tenant_id}}",
          "X-Project-ID": "{{generateTimestamp.output.project_id}}"
        },
        "body": {
          "env_id": "{{generateTimestamp.output.test_env_id}}",
          "name": "Test Environment",
          "description": "Test environment for testing",
          "is_active": false,
          "config": {
            "baseUrl": "http://test.example.com"
          },
          "variables": {
            "DB_HOST": "test-db.example.com"
          }
        },
        "timeout": 5000
      }
    },

    "activateTestEnvironment": {
      "id": "activateTestEnvironment",
      "name": "[API] Activate Test Environment",
      "type": "http",
      "dependsOn": ["createTestEnvironment"],
      "config": {
        "method": "POST",
        "url": "{{baseUrl}}/environments/{{generateTimestamp.output.test_env_id}}/activate",
        "headers": {
          "Content-Type": "application/json",
          "X-Tenant-ID": "{{generateTimestamp.output.tenant_id}}",
          "X-Project-ID": "{{generateTimestamp.output.project_id}}"
        },
        "timeout": 5000
      }
    },

    "assertActivate": {
      "id": "assertActivate",
      "name": "[Assert] Verify Activation",
      "type": "assert",
      "dependsOn": ["activateTestEnvironment"],
      "config": {
        "assertions": [
          {
            "type": "equals",
            "actual": "{{activateTestEnvironment.status}}",
            "expected": 200,
            "message": "Activate should return 200"
          }
        ]
      }
    },

    "verifyActivation": {
      "id": "verifyActivation",
      "name": "[DB] Verify Activation in Database",
      "type": "database",
      "dependsOn": ["assertActivate"],
      "config": {
        "driver": "sqlite3",
        "dsn": "{{dbPath}}",
        "queryType": "select",
        "query": "SELECT env_id, is_active FROM environments WHERE is_active = 1 AND tenant_id = ? AND deleted_at IS NULL",
        "args": ["{{generateTimestamp.output.tenant_id}}"]
      }
    },

    "assertActivationInDB": {
      "id": "assertActivationInDB",
      "name": "[Assert] Verify Only One Active Environment",
      "type": "assert",
      "dependsOn": ["verifyActivation"],
      "config": {
        "assertions": [
          {
            "type": "equals",
            "actual": "{{verifyActivation.rowCount}}",
            "expected": 1,
            "message": "Should have exactly 1 active environment per tenant"
          },
          {
            "type": "equals",
            "actual": "{{verifyActivation.rows[0].env_id}}",
            "expected": "{{generateTimestamp.output.test_env_id}}",
            "message": "Test environment should be active"
          }
        ]
      }
    },

    "deleteVariable": {
      "id": "deleteVariable",
      "name": "[API] Delete Variable",
      "type": "http",
      "dependsOn": ["assertActivationInDB"],
      "config": {
        "method": "DELETE",
        "url": "{{baseUrl}}/environments/{{generateTimestamp.output.dev_env_id}}/variables/NEW_VAR",
        "headers": {
          "Content-Type": "application/json",
          "X-Tenant-ID": "{{generateTimestamp.output.tenant_id}}",
          "X-Project-ID": "{{generateTimestamp.output.project_id}}"
        },
        "timeout": 5000
      }
    },

    "assertDeleteVariable": {
      "id": "assertDeleteVariable",
      "name": "[Assert] Verify Delete Variable",
      "type": "assert",
      "dependsOn": ["deleteVariable"],
      "config": {
        "assertions": [
          {
            "type": "equals",
            "actual": "{{deleteVariable.status}}",
            "expected": 200,
            "message": "Delete variable should return 200"
          }
        ]
      }
    },

    "deleteTestEnvironment": {
      "id": "deleteTestEnvironment",
      "name": "[API] Delete Test Environment",
      "type": "http",
      "dependsOn": ["assertDeleteVariable"],
      "config": {
        "method": "DELETE",
        "url": "{{baseUrl}}/environments/{{generateTimestamp.output.test_env_id}}",
        "headers": {
          "Content-Type": "application/json",
          "X-Tenant-ID": "{{generateTimestamp.output.tenant_id}}",
          "X-Project-ID": "{{generateTimestamp.output.project_id}}"
        },
        "timeout": 5000
      }
    },

    "deleteDevEnvironment": {
      "id": "deleteDevEnvironment",
      "name": "[API] Delete Dev Environment",
      "type": "http",
      "dependsOn": ["deleteTestEnvironment"],
      "config": {
        "method": "DELETE",
        "url": "{{baseUrl}}/environments/{{generateTimestamp.output.dev_env_id}}",
        "headers": {
          "Content-Type": "application/json",
          "X-Tenant-ID": "{{generateTimestamp.output.tenant_id}}",
          "X-Project-ID": "{{generateTimestamp.output.project_id}}"
        },
        "timeout": 5000
      }
    },

    "verifyDeletion": {
      "id": "verifyDeletion",
      "name": "[DB] Verify Deletion",
      "type": "database",
      "dependsOn": ["deleteDevEnvironment"],
      "config": {
        "driver": "sqlite3",
        "dsn": "{{dbPath}}",
        "queryType": "select",
        "query": "SELECT COUNT(*) as count FROM environments WHERE (env_id = ? OR env_id = ?) AND tenant_id = ? AND deleted_at IS NULL",
        "args": ["{{generateTimestamp.output.dev_env_id}}", "{{generateTimestamp.output.test_env_id}}", "{{generateTimestamp.output.tenant_id}}"]
      }
    },

    "assertDeletion": {
      "id": "assertDeletion",
      "name": "[Assert] Verify Deletion",
      "type": "assert",
      "dependsOn": ["verifyDeletion"],
      "config": {
        "assertions": [
          {
            "type": "equals",
            "actual": "{{verifyDeletion.rows[0].count}}",
            "expected": 0,
            "message": "Both environments should be soft deleted"
          }
        ]
      }
    },

    "cleanupTestData": {
      "id": "cleanupTestData",
      "name": "[Cleanup] Remove Test Data",
      "type": "database",
      "dependsOn": ["assertDeletion"],
      "config": {
        "driver": "sqlite3",
        "dsn": "{{dbPath}}",
        "queryType": "exec",
        "query": "DELETE FROM environments WHERE tenant_id = ?; DELETE FROM projects WHERE project_id = ?; DELETE FROM tenants WHERE tenant_id = ?",
        "args": [
          "{{generateTimestamp.output.tenant_id}}",
          "{{generateTimestamp.output.project_id}}",
          "{{generateTimestamp.output.tenant_id}}"
        ]
      }
    },

    "generateTestSummary": {
      "id": "generateTestSummary",
      "name": "[Summary] Generate Test Report",
      "type": "script",
      "dependsOn": ["cleanupTestData"],
